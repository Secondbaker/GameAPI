<p id="notice"><%= notice %></p>

<h1>Game Systems</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @game_systems.each do |game_system| %>
      <tr>
        <td><%= game_system.name %></td>
        <td><%= link_to 'Show', game_system %></td>
        <td><%= link_to 'Edit', edit_game_system_path(game_system) %></td>
        <td><%= link_to 'Destroy', game_system, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Game System', new_game_system_path %>
